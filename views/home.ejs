<%-include("partials/header")-%>
    <h1>Home</h1>

    <p>
        <%= startingContent %>
    </p>

    <%postsArray.forEach(function(post){%>
        <h1>
            <%=post.title%>
        </h1>
        <p class="truncated">
            <%=post.contentTruncated%>
                <a href="/posts/<%=post.title%>">Read More</a>
        </p>

        <%});%>

            <%-include("partials/footer")-%>